{
  "name": "zizhou-quant-platform",
  "productName": "资舟量化研究平台",
  "version": "1.8.4",
  "description": "资舟量化研究平台 - 专业的量化研究与数据管理工具",
  "author": "ZiZhou Quant Team",
  "license": "Commercial",
  "main": "dist/electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite build && electron .",
    "dev:all": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win",
    "build:win:dir": "vue-tsc --noEmit && vite build && electron-builder --win --dir",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "build:linux": "vue-tsc --noEmit && vite build && electron-builder --linux",
    "build:release": "vue-tsc --noEmit && vite build && electron-builder --win --mac -p always",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "element-plus": "^2.4.3",
    "exceljs": "^4.4.0",
    "papaparse": "^5.4.1",
    "pinia": "^2.1.7",
    "pinyin": "^4.0.0",
    "ssh2": "^1.17.0",
    "vue": "^3.3.10",
    "vue-router": "^4.2.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/papaparse": "^5.3.14",
    "@types/ssh2": "^1.15.5",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "png2icons": "^2.0.1",
    "rcedit": "^4.0.1",
    "sass": "^1.69.5",
    "sharp": "^0.34.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26"
  }
}
